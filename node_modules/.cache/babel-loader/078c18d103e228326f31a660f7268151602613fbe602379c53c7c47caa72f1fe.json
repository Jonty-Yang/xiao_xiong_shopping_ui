{"ast":null,"code":"import HomeNavBar from \"@/components/home/HomeNavBar\";\nimport Search from \"@/components/home/Search\";\nimport GoodsType from \"@/components/home/GoodsType\";\nimport Banner from \"@/components/home/Banner\";\nimport Myself from \"@/components/home/Myself\";\nimport Loves from \"@/components/home/Loves\";\nexport default {\n  name: \"Home\",\n  components: {\n    Loves,\n    Myself,\n    Banner,\n    GoodsType,\n    Search,\n    HomeNavBar\n  },\n  data() {\n    return {\n      goodsList: [],\n      tid: 0\n    };\n  },\n  methods: {\n    getTid(tid) {\n      this.tid = tid;\n      this.$axios.get('/goods/findGoodsByTid/' + tid).then(resp => {\n        let data = resp.data.data;\n        if (data.length <= 0) {\n          this.$message.error('没有该类别商品，请联系商家更新！');\n        } else {\n          this.$store.state.searchGoodsList = data;\n          this.$router.replace(\"searchGoods\");\n        }\n      });\n    },\n    getTypeName(goodsType) {\n      console.log(\"HomeBody[goodsType]\", goodsType);\n      // 更新全局搜索商品的模糊条件 回显\n      goodsType.forEach((t, i) => {\n        if (this.tid == t.tid) {\n          this.$message.error(t.name);\n        }\n      });\n      // this.$store.commit('searchGoodsCondition')\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA;AACA;AACA;AACA;AACA;AACA;AAEA;EACAA;EACAC;IAAAC;IAAAC;IAAAC;IAAAC;IAAAC;IAAAC;EAAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IAEAC;MACA;MACA,gDACAC;QACA;QACA;UACA;QACA;UACA;UAEA;QACA;MACA;IACA;IACAC;MACAC;MACA;MACAC;QACA;UACA;QACA;MACA;MACA;IACA;EACA;AACA","names":["name","components","Loves","Myself","Banner","GoodsType","Search","HomeNavBar","data","goodsList","tid","methods","getTid","then","getTypeName","console","goodsType"],"sourceRoot":"src/views","sources":["HomeBody.vue"],"sourcesContent":["<template>\r\n\r\n    <div class=\"homeBody\">\r\n        <!--搜索区-->\r\n        <div class=\"search\">\r\n            <!--logo-->\r\n            <img src=\"\" alt=\"\">\r\n            <search></search>\r\n        </div>\r\n        <!--中间主体区域-->\r\n        <div class=\"content\">\r\n            <!--左侧商品分类区域-->\r\n            <div class=\"goodsType\">\r\n                <goods-type @onSendTid=\"getTid\" @onSendTypeName=\"getTypeName\"></goods-type>\r\n            </div>\r\n            <!--中间轮播图区-->\r\n            <div class=\"banner\">\r\n                <banner></banner>\r\n            </div>\r\n            <!--右侧个人中心区-->\r\n            <div class=\"myself\">\r\n                <myself></myself>\r\n            </div>\r\n        </div>\r\n        <!--猜你喜欢区-->\r\n        <div class=\"loves\">\r\n            <!--<h2>猜你喜欢</h2>-->\r\n            <loves></loves>\r\n        </div>\r\n        <!--页脚区-->\r\n        <div class=\"footer\">\r\n            <!--<h2>页脚部分</h2>-->\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import HomeNavBar from \"@/components/home/HomeNavBar\";\r\n    import Search from \"@/components/home/Search\";\r\n    import GoodsType from \"@/components/home/GoodsType\";\r\n    import Banner from \"@/components/home/Banner\";\r\n    import Myself from \"@/components/home/Myself\";\r\n    import Loves from \"@/components/home/Loves\";\r\n\r\n    export default {\r\n        name: \"Home\",\r\n        components: {Loves, Myself, Banner, GoodsType, Search, HomeNavBar},\r\n        data() {\r\n            return {\r\n                goodsList: [],\r\n                tid: 0\r\n            }\r\n        },\r\n        methods: {\r\n\r\n            getTid(tid) {\r\n                this.tid = tid\r\n                this.$axios.get('/goods/findGoodsByTid/' + tid)\r\n                    .then(resp => {\r\n                        let data = resp.data.data;\r\n                        if (data.length <= 0) {\r\n                            this.$message.error('没有该类别商品，请联系商家更新！')\r\n                        } else {\r\n                            this.$store.state.searchGoodsList = data\r\n\r\n                            this.$router.replace(\"searchGoods\")\r\n                        }\r\n                    })\r\n            },\r\n            getTypeName(goodsType) {\r\n                console.log(\"HomeBody[goodsType]\",goodsType)\r\n                // 更新全局搜索商品的模糊条件 回显\r\n                goodsType.forEach((t, i) => {\r\n                    if (this.tid == t.tid){\r\n                        this.$message.error(t.name)\r\n                    }\r\n                })\r\n                // this.$store.commit('searchGoodsCondition')\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .homeBody {\r\n        border: 1px solid gray;\r\n        padding: 10px;\r\n    }\r\n\r\n    .search {\r\n        width: 100%;\r\n        height: 60px;\r\n        margin: 0 auto;\r\n        /*border: 1px solid red;*/\r\n        text-align: center;\r\n    }\r\n\r\n    .content > div {\r\n        display: inline-block;\r\n    }\r\n\r\n    .content {\r\n        margin-top: 30px;\r\n        width: 100%;\r\n        height: 400px;\r\n        border: 1px solid red;\r\n    }\r\n\r\n    .goodsType {\r\n        /*margin-left: 150px;*/\r\n        width: 300px;\r\n        /*border: 1px solid red;*/\r\n    }\r\n\r\n    .banner {\r\n        width: 600px;\r\n        /*border: 1px solid gray;*/\r\n        margin: 0 15px;\r\n    }\r\n\r\n    .myself {\r\n        width: 300px;\r\n        /*border: 1px solid rebeccapurple;*/\r\n    }\r\n\r\n    .goodsType, .banner, .myself {\r\n        height: 100%;\r\n        vertical-align: top;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}