{"ast":null,"code":"export default {\n  name: \"SearchGoodsByName\",\n  data() {\n    return {};\n  },\n  computed: {\n    goodsList() {\n      return this.$store.state.searchGoodsList;\n    }\n  },\n  methods: {}\n};","map":{"version":3,"mappings":"AA4BA;EACAA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;AACA","names":["name","data","computed","goodsList","methods"],"sourceRoot":"src/components/search","sources":["SearchGoodsByName.vue"],"sourcesContent":["<template>\r\n\r\n    <div class=\"box\">\r\n        <el-row>\r\n            <el-card class=\"box-card\"\r\n                     v-for=\"(goods,index) in goodsList\"\r\n                     :body-style=\"{ padding: '10px' }\">\r\n                <div class=\"goods-img\">\r\n                    <img :src=\"goods.img\" alt=\"\">\r\n                </div>\r\n                <div class=\"price\">\r\n                    <strong>{{ '￥'+ goods.dis_price }}</strong>\r\n                    <span>{{ '包邮' }}</span>\r\n                    <p>{{ goods.sales + '人付款'}}</p>\r\n                </div>\r\n                <div class=\"goods-title\">\r\n                    <span>{{ goods.name }}</span>\r\n                </div>\r\n                <div class=\"shop-name\">\r\n                    <span>{{ goods.shop_name }}</span>\r\n                    <span>{{ goods.address }}</span>\r\n                </div>\r\n            </el-card>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"SearchGoodsByName\",\r\n        data() {\r\n            return {}\r\n        },\r\n        computed: {\r\n            goodsList() {\r\n                return this.$store.state.searchGoodsList\r\n            }\r\n        },\r\n        methods: {}\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .box {\r\n        width: 1230px;\r\n        /*border: 1px solid red;*/\r\n    }\r\n\r\n    .box-card {\r\n        width: 240px;\r\n        /*height: 200px;*/\r\n        float: left;\r\n        margin-right: 30px;\r\n        /*border: 1px solid orangered;*/\r\n        position: relative;\r\n        justify-content: center;\r\n        margin-top: 10px;\r\n    }\r\n\r\n    .box-card:hover {\r\n        box-shadow: 0 0 25px #cac6c6;\r\n        border: 1px solid orangered;\r\n    }\r\n\r\n    .goods-img > img {\r\n        width: 220px;\r\n        height: 260px;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .price {\r\n        color: orangered;\r\n        font-size: 18px;\r\n    }\r\n\r\n    .price > span {\r\n        display: inline-block;\r\n        background-color: orangered;\r\n        margin-left: 5px;\r\n        font-size: 10px;\r\n        color: white;\r\n        width: 30px;\r\n        height: 16px;\r\n        line-height: 16px;\r\n        text-align: center;\r\n    }\r\n\r\n    .price > p {\r\n        display: inline-block;\r\n        float: right;\r\n        margin-top: 8px;\r\n        font-size: 12px;\r\n        color: gray;\r\n    }\r\n\r\n    .price > span, .price > strong, .price > p {\r\n        vertical-align: middle;\r\n    }\r\n\r\n    .goods-title {\r\n        font-size: 12px;\r\n        margin-top: 10px;\r\n    }\r\n\r\n    .shop-name {\r\n        font-size: 12px;\r\n        color: gray;\r\n        margin: 10px 0;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .shop-name:hover, .goods-title:hover {\r\n        color: orangered;\r\n        text-decoration: underline;\r\n    }\r\n\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}